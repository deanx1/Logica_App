    <EditText
        android:id="@+id/et_r2_3"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/et_desc_text"
        android:digits="TF"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:maxLength="1"
        app:layout_constraintBottom_toBottomOf="@+id/tv_r2_2"
        app:layout_constraintEnd_toEndOf="@+id/tv_r1_3"
        app:layout_constraintStart_toStartOf="@+id/tv_r1_3"
        app:layout_constraintTop_toTopOf="@+id/tv_r2_2" />

