        app:layout_constraintStart_toStartOf="@+id/et_r2_3"
        app:layout_constraintTop_toTopOf="@+id/tv_r3_2" />

    <TextView
        android:id="@+id/tv_r4_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/letter_f"
        app:layout_constraintBottom_toBottomOf="@+id/tv_r4_2"
        app:layout_constraintEnd_toStartOf="@+id/tv_r4_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_r3_1"
        app:layout_constraintTop_toTopOf="@+id/tv_r4_2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_r4_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="@string/letter_t"
        app:layout_constraintEnd_toEndOf="@+id/tv_r3_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_r3_2"
        app:layout_constraintTop_toBottomOf="@+id/tv_r3_2" />

    <EditText
        android:id="@+id/et_r4_3"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/et_desc_text"
        android:digits="TF"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:maxLength="1"
        app:layout_constraintBottom_toBottomOf="@+id/tv_r4_2"
        app:layout_constraintEnd_toEndOf="@+id/et_r3_3"
        app:layout_constraintStart_toStartOf="@+id/et_r3_3"
        app:layout_constraintTop_toTopOf="@+id/tv_r4_2" />

    <TextView
        android:id="@+id/tv_r5_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/letter_f"
        app:layout_constraintBottom_toBottomOf="@+id/tv_r5_2"
        app:layout_constraintEnd_toStartOf="@+id/tv_r4_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_r4_1"
        app:layout_constraintTop_toTopOf="@+id/tv_r5_2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_r5_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="@string/letter_f"
        app:layout_constraintEnd_toEndOf="@+id/tv_r4_2"
