        app:layout_constraintEnd_toStartOf="@+id/tv_r2_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_r1_1"
        app:layout_constraintTop_toTopOf="@+id/tv_r2_2"
        app:layout_constraintVertical_bias="0.0" />

